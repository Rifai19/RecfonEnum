        android:theme="@style/AppTheme">
        <activity android:name="ayyash.SarapanActivity" >

        </activity>
        <activity android:name="ayyash.PerhitunganPagi" />
        <activity android:name="ayyash.Register"></activity>
        <activity android:name="ayyash.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ayyash.Register2" />
        <activity android:name="ayyash.HalamanDepan" />
        <activity android:name="ayyash.MainMenu" />
        <activity android:name="ayyash.Login"></activity>
    </application>

</manifest>